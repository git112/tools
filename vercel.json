{
  "rewrites": [
    {
      "source": "/jobnest/api/:path*",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/api/:path*"
    },
    {
      "source": "/jobnest/auth/callback",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/auth/callback"
    },
    {
      "source": "/jobnest/auth/:path*",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/auth/:path*"
    },
    {
      "source": "/jobnest/assets/:path*",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/assets/:path*"
    },
    {
      "source": "/jobnest/:path*",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/:path*"
    },
    {
      "source": "/jobnest",
      "destination": "https://ai-powered-career-guidance-platform-nine.vercel.app/"
    },
    {
      "source": "/resume-refiner-ai/api/:path*",
      "destination": "https://resume-refiner-ai-eight.vercel.app/api/:path*"
    },
    {
      "source": "/resume-refiner-ai/auth/callback",
      "destination": "https://resume-refiner-ai-eight.vercel.app/auth/callback"
    },
    {
      "source": "/resume-refiner-ai/auth/:path*",
      "destination": "https://resume-refiner-ai-eight.vercel.app/auth/:path*"
    },
    {
      "source": "/resume-refiner-ai/assets/:path*",
      "destination": "https://resume-refiner-ai-eight.vercel.app/assets/:path*"
    },
    {
      "source": "/resume-refiner-ai/:path*",
      "destination": "https://resume-refiner-ai-eight.vercel.app/:path*"
    },
    {
      "source": "/resume-refiner-ai",
      "destination": "https://resume-refiner-ai-eight.vercel.app/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/jobnest/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/jobnest/(.*)",
      "dest": "/index.html"
    }
  ]
}